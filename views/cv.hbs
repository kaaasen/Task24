              {{#each person.qualifications}}
              <p class="light">{{this}}</p>
              {{/each}}
            </div>
          </div>
          <div class="col m2">
          </div>
        </div>
        <div class="row">
          <div class="col m2">
          </div>
          <div class="col m2">
            <h5 class = "right">Portfolio</h5>
          </div>
          <div class="col m6">
